#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/html-validate@6.1.4_jest@27.4.5/node_modules/html-validate/bin/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/html-validate@6.1.4_jest@27.4.5/node_modules/html-validate/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/html-validate@6.1.4_jest@27.4.5/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/node_modules:/mnt/c/Users/Gebruiker/Desktop/node_modules:/mnt/c/Users/Gebruiker/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/html-validate/bin/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/html-validate/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/html-validate@6.1.4_jest@27.4.5/node_modules/html-validate/bin/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/html-validate@6.1.4_jest@27.4.5/node_modules/html-validate/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/html-validate@6.1.4_jest@27.4.5/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/.pnpm/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/node_modules:/mnt/c/Users/Gebruiker/Desktop/node_modules:/mnt/c/Users/Gebruiker/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/html-validate/bin/node_modules:/mnt/c/Users/Gebruiker/Desktop/HackYourHomework/Homework-class35/node_modules/html-validate/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-validate/bin/html-validate.js" "$@"
else
  exec node  "$basedir/../html-validate/bin/html-validate.js" "$@"
fi
